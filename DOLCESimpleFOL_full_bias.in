(all X all S (hasSource(X,S) -> (inf(X) & (bob(S) | cob(S))))).
(all X all T (hasTarget(X,T) -> (inf(X) & bob(T)))).

(all X all O (hasObject(X,O) -> (bob(X) & ed(O)))).
(all X all Y (hasConcept(X,Y) -> (bob(X) & c(Y)))).
(all X all T (hasTime(X,T) -> (bob(X) & t(T)))).


