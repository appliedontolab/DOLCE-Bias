(all Z (cob(Z) -> (exists X exists Y exists T (-(X=Y) & bob(X) & (bob(Y) | cob(Y)) & sumt(Z,X,Y,T))))).

(all X (bob(X) <-> (ob(X) & (exists A exists O exists C exists T (hasAgent(X,A) & hasObject(X,O) & hasConcept(X,C) & hasTime(X,T)))))).

